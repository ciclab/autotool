      (flag signed)
    )
    (imm5
      (width 5)
      (flag signed)
    )
    ( int128
      ( width 128 )
      ( flag signed)
    )
    ( int32
      ( width 32 )
      ( flag signed )
    )
  )
  (pipe_line
    ( state_p imm11 )
    ( binary_p int128 )
    ( valA_p int32 )
    ( valB_p int32 )
    ( valC_p int32 )
    ( pc_p int32 )
    ( rd_p imm5 )
    ( rs_p imm5 )
    ( rt_p imm5 )
  )
  (instruction
    (top instrs)
    (instrs
      (= i  slots_4 )
      (do)
      (code i)
      (binary i)
      (vliw 1 0)
    )
    (slot_1
      (= i slot0)
      (do)
      (binary '0' i)
      (code i)
    )
    (empty
      (do)
      (binary '')
      (code '')
    )
    (slots_4
      (= i slot0 empty)
      (= j slot1 empty)
      (= k slot2 empty)
      (= l slot3 empty)
      (pack i j k l)
    )
    (slot0
      (= i NOP RET RTT JC JNC JMP CALL VSEL20 JMPR CALLR
        MOVC2G MOVG2C SCALER V10BIT V20BIT V40BIT VMOVCV2V
	V10BIT11 V20BIT10 V40BIT01 V11 VFMUL VMAC VCMUL VCMULI40
	VCMAC MOVI
	 CMP CMPU CH MOVG2G MOVV2V  VMOVR VNOT VABS VSUM
	LOOP BFST TEST BST ADDI BFEXT MOVV2G MOVG2V 
      )
      (do)
      (code i)
      (binary '000' i)
    )
    (slot1
      (= i NOP RET RTT JC JNC JMP CALL VSEL20 JMPR CALLR
        MOVC2G MOVG2C SCALER V10BIT V20BIT V40BIT VMOVCV2V
	V10BIT11 V20BIT10 V40BIT01 V11 VFMUL VMAC VCMUL VCMULI40
	VCMAC MOVI
	 CMP CMPU CH MOVG2G MOVV2V  VMOVR VNOT VABS VSUM
	LOOP BFST TEST BST ADDI BFEXT MOVV2G MOVG2V
      )
      (do)
      (code i)
      (binary '001' i)
    )
    (slot2
      (= i NOP 
             VMOVCV2V
	 VLOADO SLOADO VSTOREO SSTOREO LOAD LOADU VLOAD 
	VLOADUR VLOADU VLOADR STORE STOREU STOREI VSTORE STOREUU
	VSTOREEU  MOVG2G MOVV2V  VMOVR  MOVI
      )
      (do)
      (code i)
      (binary '010' i)
    )
    (slot3
      (= i NOP 
             VMOVCV2V
	 VLOADO SLOADO VSTOREO SSTOREO LOAD LOADU VLOAD
	VLOADUR VLOADU VLOADR STORE STOREU STOREI VSTORE STOREUU
	VSTOREEU  MOVG2G MOVV2V  VMOVR MOVI
      )
      (do)
      (code i)
      (binary '011' i)
    )
    (NOP
      (do)
      (code 'nop')
      (binary '000' '000000' '000000' '000000' '00000000') 
    )
    (RET
      (do)
      (code 'ret')
      (binary '000' '000000' '000000' '000000' '000001' '00')
    )
    (RTT
      (do)
      (code 'rtt')
      (binary '000' '000000' '000000' '000000' '000010' '00')
    )
    (VSEL20
      (= rd VR_E)
      (= rs VR_E)
      (= rt GR_E)
      (= rs1 GR_E)
      (do)
      (code 'vsel20' ' ' rd ' ' rs ' ' rt ' ' rs1)
      (binary '000' rd rs '0' rt '0'  rs1 '10')
    )
    (JMP
      (= a addr21Rel)
      (do
      // 表示在执行阶段修改pc的值
        (execute_s
	// 用一个cycle
	  (cycle 1)
	  (= fetch_s.pc a)
	  (call decode_s.abort)
	)
      )
      (code 'jmp' ' ' a)
      (binary '000' a '0' '00' '01')
    )
    (JC
      (= a addr21Rel)
      (do
        (execute_s
	  (cycle 1)
	  (if (& memacc_s.state 1)
   	    (
	      (= fetch_s.pc a)
	      (call decode_s.abort)
	    )
	  )
	)
      )
      (code 'jc' ' ' a)
      (binary '000' a '0' '01' '01')
    )
    (JNC
      (= a addr21Rel)
      (do)
      (code 'jnc' ' ' a)
      (binary '000' a '0' '10' '01')
    )
    (CALL
      (= a addr21Abs)
      (do
      )
      (code 'call' ' ' a)
      (binary '000' a '0' '11' '01')
    )
    (JMPR
      (do)
      (= rs GR_E)
      (code 'jmpr' ' '  rs)
      (binary '000' '000000' '0' rs '000000' '000000' '11')
    )
    (CALLR
      (do)
      (= rs GR_E)
      (code 'callr' ' ' rs)
      (binary '000' '000000' '0' rs '000000' '000001' '11')
    )
    (MOVG2C
      (do)
      (= rs GR_E)
      (code 'movg2c' ' ' rs)
      (binary '000' '000000' '0' rs '000000' '000010' '11')
    )
    (MOVC2G
      (do)
      (= rd GR_E)
      (code 'movc2g' ' ' rd)
      (binary '000' '0' rd '000000' '000000' '000011' '11')
    )
    (SCALER
      (do)
      (= i SCALER_E)
      (= rd GR_E)
      (= rs GR_E)
      (= rt GR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' '0' rd '0' rs '0' rt '0000' i)
    )
    (V10BIT
      (do)
      (= i V10BIT_E)
      (= rd GR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' '0' rd rs rt '001' i '11')
    )
    (V20BIT
      (do)
      (= i V20BIT_E)
