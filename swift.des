    (V11
      (do)
      (= i V11_E)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '0100' i '00')
    )
    (VFMUL
      (do)
      (= i VFMUL_E)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '10' '0000' i)
    )
    (VMAC
      (do)
      (= i VMAC_E)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '10' '0100' i)
    )
    (VCMUL
      (do)
      (= i VCMUL_E)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '10' '1000' i)
    )
    (VCMULI40
      (do)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code 'vcmuli40' ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '10' '1001' '01') /*should be generalized to VCMUL*/
    )
    (VCMAC
      (do)
      (= i VCMAC_E)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '10' '1100' i)
    )
    (VLOADO
      (do)
      (= i VLOADO_E)
      (= rd VR_E)
      (= rs GR_E)
      (= rt BAR_OFF_MR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd '0' rs rt '11' '1000' i)
    )
    (SLOADO
      (do)
      (= i SLOADO_E)
      (= rd GR_E)
      (= rs GR_E)
      (= rt BAR_OFF_MR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' '0' rd '0' rs rt '11' '0000' i)
    )
    (VSTOREO
      (do)
      (= i VSTOREO_E)
      (= rd VR_E)
      (= rs GR_E)
      (= rt BAR_OFF_MR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' rd '0' rs rt '11' '1100' i)
    )
    (SSTOREO
      (do)
      (= i SSTOREO_E)
      (= rd GR_E)
      (= rs GR_E)
      (= rt BAR_OFF_MR_E)
      (code i ' ' rd ' ' rs ' ' rt)
      (binary '001' '0' rd '0' rs rt '11' '0100' i)
    )
    (LOAD
      (do)
      (= i LOAD_E)
      (= rd GR_E)
      (= rs GR_E)
      /*(= imm imm9)*/
      (= imm addr9Abs)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '010' '0' rd '0' rs imm '000' i)
    )
    (LOADU
      (do)
      (= i LOADU_E)
      (= rd GR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '010' '0' rd '0' rs imm '010' i)
    )
    (VLOAD
      (do)
      (= i VLOAD_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '010' rd '0' rs imm '100' i)
    )
    (VLOADR
      (do)
      (= i VLOADR_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '010' rd '0' rs imm '101' i)
    )
    (VLOADU
      (do)
      (= i VLOADU_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '010' rd '0' rs imm '110' i)
    )
    (VLOADUR
      (do)
      (= i VLOADUR_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '010' rd '0' rs imm '111' i)
    )
    (STORE
      (do)
      (= i STORE_E)
      (= rd GR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '011' '0' rd '0' rs imm '000' i)
    )
    (STOREU
      (do)
      (= i STOREU_E)
      (= rd GR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '011' '0' rd '0' rs imm '010' i)
    )
    (STOREI
      (do)
      (= i STOREI_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '011' rd '0' rs imm '100' i)
    )
    (VSTORE
      (do)
      (= i VSTORE_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '011' rd '0' rs imm '101' i)
    )
    (STOREUU
      (do)
      (= i STOREUU_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '011' rd '0' rs imm '110' i)
    )
    (VSTOREEU
      (do)
      (= i VSTOREEU_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm9)
      (code i ' ' rd ' ' rs ' ' imm)
      (binary '011' rd '0' rs imm '111' i)
    )
    (CMP
      (do)
      (= i CMP_E)
      (= rs GR_E)
      (= rt GR_E)
      (code i ' ' rs ' ' rt)
      (binary '100' '000000' '0' rs '0' rt '00000' i)
    )
    (CMPU
      (do)
      (= i CMPU_E)
      (= rs GR_E)
      (= rt GR_E)
      (code i ' ' rs ' ' rt)
      (binary '100' '000000' '0' rs '0' rt '00001' i)
    )
    (CH
      (do)
      (= i CH_E)
      (= rd GR_E)
      (= rs GR_E)
      (code i ' ' rd ' ' rs)
      (binary '100' '0' rd '0' rs '000000'  '010001' i)
    )
    (MOVG2G
      (do)
      (= rd GR_OFF_BAR_MR_E)
      (= rs GR_OFF_BAR_MR_E)
      (code 'movg2g' ' ' rd ' ' rs)
      (binary '100' rd rs '000000'  '100000' '00')
    )
    (MOVV2V
      (do)
      (= rd VR_E)
      (= rs VR_E)
      (code 'movv2v' ' ' rd ' ' rs)
      (binary '100' rd rs '000000'  '101110' '00')
    )
    (VMOVR
      (do)
      (= i VMOVR_E)
      (= rd VR_E)
      (= rs GR_E)
      (code i ' ' rd ' ' rs)
      (binary '100' rd '0' rs '000000'  '101001' i)
    )
    (VNOT
      (do)
      (= rd VR_E)
      (= rs VR_E)
      (code 'vnot' ' ' rd ' ' rs)
