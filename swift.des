    )
    (VABS
      (do)
      (= i VABS_E)
      (= rd VR_E)
      (= rs VR_E)
      (code i ' ' rd ' ' rs)
      (binary '100' rd rs '000000'  '111110' i)
    )
    (VSUM
      (do)
      (= i VSUM_E)
      (= rd VR_E)
      (= rs VR_E)
      (code i ' ' rd ' ' rs)
      (binary '100' rd rs '000000'  '111111' i)
    )
    (LOOP
      (do)
      (= imm imm16)
      (= rd GR_E)
      (code 'loop' ' ' rd ' ' imm)
      (binary '101' '0' rd imm '0000' )
    )
    (MOVI
      (= i MOVI_E)
      (= rd GR_OFF_BAR_MR_E )
      (= imm imm16)
      (do
        (decode_s
	  (= valA imm )
	  (= valB ([] gr_r rd) )
	)
	(execute_s
          (if (eq i 0 )
	    (= valA ( | ( & valA 65535 ) (<< valB 16 ) ) )
	    (= valA ( | ( & valA 4294901760 ) valB ) )
	  )
	)
	(writeback_s
	  // 认为只支持gr,由于目前的寄存器定义过于奇特
	  (= ([] gr_r rd ) valA0 )
	)
      )
      (code i ' ' rd ' ' imm )
      (binary '101' rd imm '0' i '01' )
    )
    (TEST
      (= i TEST_E)
      (= rs GR_E)
      (= imm imm9)
      (do)
      (code i ' ' rs ' ' imm )
      (binary '101' '000000' '0' rs imm i '10' )
    )
    (BST
      (= i BST_E)
      (= rd GR_E)
      (= imm imm5)
      (do)
      (code i ' ' rd ' ' imm )
      (binary '101' '0' rd '000000' imm '0000' '00' i '11' )
    )
    (ADDI
      (= i ADDI_E)
      (= rd GR_E)
      (= rs GR_E)
      (= imm imm11)
      (do
        (decode_s
	  ( clock 1 )
	  (= valB ([] gr rs) )
	  (= valC i)
	)
        (execute_s
	  ( clock 1 )
	  (= valA1 (+ valB valC)
	  )
	)
	(writeback_s
	  (clock 1)
	  ( = ([] gr rs) valC )
	)
      )
      (code i ' ' rd ' ' rs ' ' imm )
      (binary '110' '0' rd '0' rs imm i '00' )
    )
    (BFEXT
      (= i BFEXT_E)
      (= rd GR_E)
      (= rs GR_E)
      (= imm imm10)
      (do)
      (code i ' ' rd ' ' rs ' ' imm )
      (binary '110' '0' rd '0' rs imm '0' i '01' )
    )
    (BFST
      (= rd GR_E)
      (= rs GR_E)
      (= imm imm10)
      (do)
      (code 'bfst' ' ' rd ' ' rs ' ' imm )
      (binary '110' '0' rd '0' rs imm '00' '10' )
    )
    (MOVV2G
      (= i MOVV2G_E)
      (= rd GR_E)
      (= rs VR_E)
      (= imm imm4)
      (do)
      (code i ' ' rd ' ' rs ' ' imm )
      (binary '110' '0' rd rs imm '0000' '01' i '11' )
    )
    (MOVG2V
      (= i MOVG2V_E)
      (= rd VR_E)
      (= rs GR_E)
      (= imm imm4)
      (do)
      (code i ' ' rd ' ' rs ' ' imm )
      (binary '110' rd '0' rs imm '0000' '10' i '11' )
    )
  )
)
