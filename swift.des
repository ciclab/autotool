    )
    (VMOVCV2V
      (do)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code 'vmovcv2v' ' ' rd ' ' rs ' ' rt)
      (binary '001' rd rs rt '011' '00000')
    )
    (V10BIT11
      (do)
      (= i V10BIT11_E)
      (= rd VR_E)
      (= rs VR_E)
      (= rt VR_E)
      (code i ' ' rd ' ' rs ' ' rt)
